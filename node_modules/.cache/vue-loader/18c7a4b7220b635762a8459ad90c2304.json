ipt>\nimport axios from \"axios\";\nimport { useCookies } from \"vue3-cookies\";\n\nexport default {\n  name: \"LoginComponent\",\n  setup() {\n    const { cookies } = useCookies();\n    return {\n      cookies,\n    };\n  },\n  data() {\n    return {\n      email: [],\n      password: null,\n      showPassword: false,\n      isHidden: false,\n      isVisible: true,\n    };\n  },\n  mounted() {\n    this.checkToken();\n  },\n  computed: {\n    buttonLabel() {\n      return this.showPassword ? \"Hide\" : \"Show\";\n    },\n  },\n  methods: {\n    async login() {\n      try {\n        var data = await axios.post(\n          \"http://auth.merahputihacademy.com/api/v1/login?email=\" +\n            this.email +\n            \"&password=\" +\n            this.password\n        );\n        console.log(data);\n        this.cookies.set(\"name\", data.data.data.name);\n        this.cookies.set(\"origin\", data.data.data.origin);\n        this.cookies.set(\"X-GAN-TOKEN\", data.data.data.token);\n        this.$router.push(\"/dashboard\");\n      } catch (error) {\n        // window.alert(\"Akun Tidak Ditemukan\");\n        this.isVisible = false;\n        this.email = \"\";\n        this.password = \"\";\n        setTimeout(() => {\n          this.isVisible = true;\n        }, 4000);\n      }\n    },\n    showFeedback({ suggestions, warning }) {\n      //   console.log(suggestions);\n      //   console.log(warning);\n    },\n    showScore(score) {\n      //   console.log(score);\n    },\n\n    togglePassword() {\n      var x = document.getElementById(\"password\");\n      if (x.type === \"password\") {\n        x.type = \"text\";\n        this.isHidden = true;\n      } else {\n        x.type = \"password\";\n        this.isHidden = false;\n      }\n    },\n    checkToken() {\n      if (this.cookies.get(\"X-GAN-TOKEN\")) {\n        this.$router.push(\"/dashboard\");\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n* {\n  transition: ease-in-out 0.2;\n}\n\n.loginpage {\n  -ms-overflow-style: none; /* Internet Explorer 10+ */\n  scrollbar-width: none;\n}\n\n.container-2 p {\n  margin: 0;\n}\n\n.login-container {\n  display: flex;\n  flex-direction: row;\n  width: 40%;\n  border: 1px solid #c4c4c4;\n  border-radius: 10px;\n  margin: 0;\n}\n\n.login-container-border-wrong {\n  border: 1px solid #8d191c;\n}\n\n.login-sub-container {\n  padding: 70px;\n  flex: 1;\n  margin: 0;\n}\n\n.wrapper {\n  // padding: 30px 20px;\n  // overflow: hidden;\n  align-items: center;\n}\n\n#toast {\n  // width: 380px;\n  // height: 80px;\n  padding: 20px;\n  background-color: #ffffff;\n  box-shadow: 0 10px 20px rgba(75, 50, 50, 0.05);\n  border-left: 8px solid red;\n  border-radius: 7px;\n  display: grid;\n  grid-template-columns: 1.2fr 6fr 0.5fr;\n  // transform: translate(400px);\n  transition: 0.2s;\n}\n\n.isVisible {\n  display: none;\n  transition: ease-in-out 0.2;\n}\n\n#login {\n  height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.cstm-fc {\n  border: none !important;\n}\n\n.custom-input {\n  display: flex;\n  flex-direction: row;\n  background-color: #fff;\n  border: 1px solid #bbb;\n  border-radius: 3px;\n  align-items: center;\n  justify-content: center;\n}\n\n#gambarOneId {\n  //    border: 2px solid ;\n  border-radius: 10px;\n  background-color: whitesmoke;\n  width: 50%;\n  height: fit-content;\n  align-items: center;\n}\n\n#psiKotesttitle {\n  color: #8d191c;\n  font-weight: bold;\n  font-weight: 800;\n  margin-bottom: 50px;\n}\n\n#buttonToggle {\n  margin-left: 608px !important;\n  margin-bottom: 20px;\n  margin-top: -26px;\n  width: 35px;\n  height: 35px;\n  border: none !important;\n  //   background-color: white;\n}\n\n.spanField {\n  position: absolute;\n  right: 185px;\n  transform: translate(0, -50%);\n  top: 373px;\n  cursor: pointer;\n}\n\n#rest-1 {\n  color: #8d191c !important;\n  font-weight: 700;\n}\n\n#exampleInputEmail1 {\n  width: 638px;\n  height: 38px;\n}\n\n@media only screen 