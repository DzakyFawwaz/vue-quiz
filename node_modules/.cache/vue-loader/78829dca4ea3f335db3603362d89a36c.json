ord: null,\n      showPassword: false,\n      isHidden: false,\n      isVisible: true,\n    };\n  },\n  mounted() {\n    this.checkToken();\n  },\n  computed: {\n    buttonLabel() {\n      return this.showPassword ? \"Hide\" : \"Show\";\n    },\n  },\n  methods: {\n    async login() {\n      try {\n        var data = await axios.post(\n          \"/login\",\n          {\n            email: this.email,\n            password: this.password,\n          },\n          {\n            headers: {\n              destination: \"Auth\",\n            },\n          }\n        );\n        // console.log(data);\n        this.cookies.set(\"name\", data.data.data.name);\n        this.cookies.set(\"origin\", data.data.data.origin);\n        this.cookies.set(\"X-GAN-TOKEN\", data.data.data.token);\n        this.$router.push(\"/dashboard\");\n      } catch (error) {\n        // window.alert(\"Akun Tidak Ditemukan\");\n        this.isVisible = false;\n        this.email = \"\";\n        this.password = \"\";\n        setTimeout(() => {\n          this.isVisible = true;\n        }, 4000);\n      }\n    },\n    togglePassword() {\n      var x = document.getElementById(\"password\");\n      if (x.type === \"password\") {\n        x.type = \"text\";\n        this.isHidden = true;\n      } else {\n        x.type = \"password\";\n        this.isHidden = false;\n      }\n    },\n    checkToken() {\n      if (this.cookies.get(\"X-GAN-TOKEN\")) {\n        this.$router.push(\"/dashboard\");\n      }\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n* {\n  transition: ease-in-out 0.2;\n}\n\n// .toast {\n//   position: absolute;\n//   top: 0;\n//   right: 0;\n//   z-index: 100;\n// }\n\n.loginpage {\n  // position: relative;\n  -ms-overflow-style: none; /* Internet Explorer 10+ */\n  scrollbar-width: none;\n  height: 100vh;\n  overflow: hidden;\n  object-fit: cover;\n  background-size: cover;\n  background-image: url(\"data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' version